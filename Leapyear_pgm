def Isitleapyear(yr):
  if yr%2 == 0:
    if yr%400 == 0:
      if yr%100 == 0:
        if yr%4 == 0:
          return True
  else 
    return False
    
test_year = [1900, 2000, 2016, 2008]
test_results = ["False", "True", "True", "False"]
for yr in test_year:
  for i in test_results:
    if test_results[i] == Isitleapyear(yr)
      print("ok")
    else
      print("Failed")
