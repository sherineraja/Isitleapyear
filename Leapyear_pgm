def Isitleapyear(yr):
    if (yr % 4 == 0):
        if (yr % 100 == 0):
            if (yr % 400 == 0):
                print("True") 
            else:
                print("False") 
        else:
            print("True")
    else:
        print("False")
yr = int(input("Enter a year from 1900 to 2100:" ))
Isitleapyear(yr)
